{
    "name": "vscode-idl",
    "displayName": "idl",
    "description": "IDL language extension for Microsoft Visual Studio Code",
    "version": "0.0.1",
    "publisher": "kuravih",
    "license": "MIT",
    "author": {
        "email": "kuravih@outlook.com",
        "name": "Kuravi Hewawasam",
        "url": "http://kuravih.github.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kuravih/vscode-idl"
    },
    "preview": true,
    "homepage": "https://github.com/kuravih/vscode-idl",
    "icon": "images/logo.svg",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "idl",
            "aliases": ["IDL", "idl"],
            "extensions": [".pro"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "idl",
            "scopeName": "source.idl",
            "path": "./syntaxes/idl.tmLanguage"
        }]
    }
}